    "def getDataMultipleYears(startYear, endYear):\n",
    "    frames = []\n",
    "    for timeperiod in createTimeperiods(startYear, endYear):\n",
    "        frames.append(getData(timeperiod, client_id))\n",
    "\n",
    "    return pd.concat(frames, ignore_index = True)\n",
